void tftdrawTextSmall(int x, int y, char c)
{
  switch (c) {
    case '0':
      tftdrawSmall0(x, y);
    break;
    case '1':
      tftdrawSmall1(x, y);
    break;
    case '2':
      tftdrawSmall2(x, y);
    break;
    case '3':
      tftdrawSmall3(x, y);
    break;
    case '4':
      tftdrawSmall4(x, y);
    break;
    case '5':
      tftdrawSmall5(x, y);
    break;
    case '6':
      tftdrawSmall6(x, y);
    break;
    case '7':
      tftdrawSmall7(x, y);
    break;
    case '8':
      tftdrawSmall8(x, y);
    break;
    case '9':
      tftdrawSmall9(x, y);
    break;
    case '-':
      tftdrawNegative(x, y);
    break;
    default:
      tftdrawSmallBlank(x, y);
    break;
  }
}

