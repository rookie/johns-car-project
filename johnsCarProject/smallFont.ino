//Symbols

void tftdrawDegree(int x, int y)
{
  tft.drawBitmap(x, y,  icon_degree, 16, 12, fgColor);
}
void tftdrawF(int x, int y)
{
  tft.drawBitmap(x, y,  icon_f, 16, 27, fgColor);
}
void tftdrawC(int x, int y)
{
