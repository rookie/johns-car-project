}

void tftdrawColon(int x, int y)
{
  tft.drawBitmap(x, y,  icon_colon, 8, 19, fgColor);
}
void tftdrawSmallBlank(int x, int y)
{
  tft.fillRect( x, y, 16, 27, bgColor);
}


